{
	"type": "multiple",
	"name": "Mycorrhiznth Navigator Oranel of Gravitational Rearrangement reverse gravity",
	"description": "Mycorrhiznth Navigators organel of gravitational rearrangement can be used to invert the direction they fall",
	"gravmode": {
		"type": "origins:toggle",
		"active_by_default": false,
		"key": {
			"key": "key.origins.secondary_active"
		}
	},
	"gravmodeOn": {
		"type": "origins:active_self",
		"entity_action": {
			"type": "origins:toggle",
			"power": "origins:reversegravity"
		},
		"cooldown": 1,
		"hud_render": {
			"should_render": false
		},
		"key": {
			"key": "key.origins.secondary_active",
			"continuous": true
		},"condition": {

			"type": "origins:power_active",
			"power": "*:*_gravmode"

		}
	},
	"gravmodeOFF": {
		"type": "origins:active_self",
		"entity_action": {
			"type": "origins:toggle",
			"power": "origins:normalgravity"
		},
		"cooldown": 1,
		"hud_render": {
			"should_render": false
		},
		"key": {
			"key": "key.origins.secondary_active",
			"continuous": true
		},"condition": {

			"type": "origins:power_active",
			"power": "*:*_gravmode"

		}

	},
	"normalgravity": {
		"type": "origins:modify_velocity",
		"modifier": {
			"value": 1,
			"operation": "multiply_total"
		},
		"axes": [
			"y"
		]
	},
	"reversegravity": {
		"type": "origins:modify_velocity",
		"modifier": {
			"value": -1,
			"operation": "multiply_total"
		},
		"axes": [
			"y"
		]
	}
}